  "scripts": {
    "start-easydev": "EASY_DEV=true nodemon nodemon ",
    "start-inmemory": "INMEMORY=true nodemon",
    "start": "nodemon",
    "test": "jest --coverage --config=config/jest.all.js --runInBand",
    "unit-test-watch": "npm run unit-test -- --watch",
    "integ-test-watch": "npm run integ-test -- --watch",
    "unit-test": "jest --coverage --config=config/jest.unit.js",
    "integ-test": "jest --coverage --config=config/jest.integ.js --runInBand",
    "clean-dist": "rm -f -r -d dist",
    "clean-dist-windows": "rmdir /s dist",
    "knex-new-migration-file": "knex migrate:make gratitudeapp --knexfile=config/knexfile.js",
    "knex-migration-latest-dev": "knex migrate:latest --env development --knexfile=config/knexfile.js",
    "apply-sql-seeds-dev": "knex seed:run --env development --knexfile=config/knexfile.js",
    "eslint:fix": "eslint --fix src"
  },
