{
  "name": "node-tdd-cleanarchi-9-fev-2021",
  "version": "1.0.0",
  "description": "Backend Node / Typescript pour la formation du 9 et 10 f√©vrier 2021",
  "main": "src/adapters/primary/rest/server.ts",
  "scripts": {
    "build-start": "npm run clean-dist && webpack --config config/webpack.config.js",
    "postinstall": "npm run clean-dist && webpack --config config/webpack.config-prod.js",
    "build-start-easydev": "npm run clean-dist && EASY_DEV=true webpack --config config/webpack.config.js",
    "build-start-inmemory": "npm run clean-dist && INMEMORY=true webpack --config config/webpack.config.js",
    "build-start-windows": "npm run clean-dist-windows && webpack --config config/webpack.config.js",
    "build-start-windows-inmemory": "npm run clean-dist-windows && INMEMORY=true webpack --config config/webpack.config.js",
    "start": "nodemon dist/server.js",
    "start-inmemory": "INMEMORY=true nodemon dist/server.js",
    "test": "jest --coverage --config=config/jest.all.js --runInBand",
    "unit-test-watch": "npm run unit-test -- --watch",
    "integ-test-watch": "npm run integ-test -- --watch",
    "unit-test": "jest --coverage --config=config/jest.unit.js",
    "integ-test": "jest --coverage --config=config/jest.integ.js --runInBand",
    "clean-dist": "rm -f -r -d dist",
    "clean-dist-windows": "rmdir /s dist",
    "knex-new-migration-file": "knex migrate:make gratitudeapp --knexfile=config/knexfile.js",
    "knex-migration-latest-dev": "knex migrate:latest --env development --knexfile=config/knexfile.js",
    "apply-sql-seeds-dev": "knex seed:run --env development --knexfile=config/knexfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mica16/node-tdd-cleanarchi-9-fev-2021.git"
  },
